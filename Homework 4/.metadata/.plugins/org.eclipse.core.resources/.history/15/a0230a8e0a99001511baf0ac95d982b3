import java.util.Scanner;
public class NumberSearch {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int[] a = {7,23,4,6,9,4,1};
		System.out.println("enter the numbe" );
		int b = sc.nextInt();
		int c = 0;
		
		
		for (int i = 0; i < a.length; i++) {
			
			if(a[i]==b);
			System.out.print(i);
		}
			
			
	        
		
		
		
		
		
		
	}
}
		
	 
	 

		
		
		
		
	
		
	


