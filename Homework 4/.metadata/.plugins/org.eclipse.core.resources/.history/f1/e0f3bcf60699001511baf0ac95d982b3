import java.util.Scanner;
public class NumberSearch {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int[] a = new int[40];
		int b = sc.nextInt();
		
		for (int i = 0; i < a.length; i++) {
			if(a[i]==b);
			
			System.out.println(i);
		}
		
		
		
		
		
	}
}
		
	 
	 

		
		
		
		
	
		
	


