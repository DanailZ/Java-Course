
public class ReverseMasive {
   
	public static void main(String[] args) {
		int[]a = {3,6,1,7,8,4,2,8,4,9};
		
		for (int i = 0; i < a.length /2;i++) {
			int b = a[i];
			a[i]=a[a.length-1];
			a[a.length-1] = b;
			
		}
		for (int i = 0; i < a.length; i++) {
			
			System.out.println(a[i]);
		}
	}

}
