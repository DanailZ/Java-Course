import java.*;
public class TestArray {

	public static void main(String[]args){
		int[] a  = {5,7};
		int[] b = {3,8};
		int n = 40;
		
		for (int i = 0; i < a.length; i++) {
			for (int j = 0; j < b.length; j++) {
				if (n % a[i]==0 && n%b[j]!=0){
					System.out.println("good"+ a[i]);
					
				
				}
				
			}
		}
	}
}
