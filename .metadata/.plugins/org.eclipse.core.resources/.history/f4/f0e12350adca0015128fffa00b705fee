import java.*;
public class TestArray {

	public static void main(String[]args){
		int[] a  = {5,7};
		int[] b = {3,8};
		int n = 40;
		
		for (int i = 0; i < a.length; i++) {
			for (int j = 1; j < b.length; j++) {
				for (int j2 = 0; j2 < b.length; j2++) {
				if (i % a[j]==0 && i%b[j2] !=0){
					
					System.out.println(i);
		      
				}
				}
			}
		}
	}
}
